{
	"name": "run-taro",
	"version": "1.0.0",
	"private": true,
	"description": "run of Taro",
	"templateInfo": {
		"name": "redux",
		"typescript": false,
		"css": "sass"
	},
	"scripts": {
		"build:weapp": "taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:rn": "taro build --type rn",
		"build:qq": "taro build --type qq",
		"build:quickapp": "taro build --type quickapp",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch",
		"format": "yarn prettier && yarn lint --fix",
		"prettier": "prettier --write './src/**/*'",
		"lint:fix": "eslint --fix --ext .js src && npm run lint:style",
		"lint": "eslint --ext .js src && npm run lint:style",
		"lint:style": "stylelint 'src/**/*.scss' --syntax scss",
		"precommit": "lint-staged"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"author": "Tony <1362106500@qq.com>",
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "^7.7.7",
		"@tarojs/components": "3.0.21",
		"@tarojs/react": "3.0.21",
		"@tarojs/runtime": "3.0.21",
		"@tarojs/taro": "3.0.21",
		"@tarojs/taro-h5": "3.0.21",
		"async-validator": "^3.2.4",
		"classnames": "^2.2.6",
		"dva-core": "^2.0.2",
		"jwt-decode": "^2.2.0",
		"lodash": "^4.17.11",
		"nervjs": "^1.5.7",
		"path-to-regexp": "^3.0.0",
		"qr.js": "^0.0.0",
		"react": "^16.10.0",
		"react-dom": "^16.10.0",
		"react-redux": "^7.0.0",
		"redux": "^4.0.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"taro-ui": "^3.0.0-alpha"
	},
	"devDependencies": {
		"@babel/core": "^7.8.0",
		"@tarojs/mini-runner": "3.0.21",
		"@tarojs/webpack-runner": "3.0.21",
		"@types/react": "^16.0.0",
		"@types/webpack-env": "^1.13.6",
		"babel-core": "^6.26.3",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.1.0",
		"babel-plugin-lodash": "^3.3.4",
		"babel-plugin-transform-remove-console": "^6.9.4",
		"babel-preset-env": "^1.7.0",
		"babel-preset-taro": "3.0.21",
		"eslint": "^6.8.0",
		"eslint-config-taro": "3.0.21",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-react-hooks": "^1.6.1",
		"eslint-plugin-taro": "3.0.21",
		"lint-staged": "^7.2.2",
		"lodash-webpack-plugin": "^0.11.5",
		"prettier": "^1.17.0",
		"stylelint": "9.3.0",
		"stylelint-config-prettier": "^4.0.0",
		"stylelint-config-standard": "^18.0.0",
		"webpack": "^4.43.0",
		"webpack-bundle-analyzer": "^3.3.2"
	},
	"lint-staged": {
		"**/*.{js,jsx,scss}": [
			"prettier --write",
			"git add"
		],
		"**/*.{js,jsx}": "npm run lint-staged:js",
		"**/*.scss": "stylelint --syntax scss"
	}
}
